{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "logger": {
      "type": "object",
      "default": null
    },
    "bot": {
      "type": "object",
      "properties": {
        "egg_done": {
          "type": "string",
          "default": "change this part"
        },
        "commands": {
          "type": "object",
          "default": {}
        },
        "control": {
          "type": "object",
          "properties": {
            "prefix": {
              "type": "string",
              "default": "!"
            }
          },
          "required": [
            "prefix"
          ]
        },
        "event": {
          "type": "object",
          "properties": {
            "user": {
              "type": "object",
              "properties": {
                "join": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                },
                "update": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                },
                "leave": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                }
              },
              "required": [
                "join",
                "update",
                "leave"
              ]
            },
            "message": {
              "type": "object",
              "properties": {
                "new": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                },
                "delete": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                },
                "edit": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                }
              },
              "required": [
                "new",
                "delete",
                "edit"
              ]
            },
            "voice": {
              "type": "object",
              "properties": {
                "join": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                },
                "leave": {
                  "type": "object",
                  "properties": {
                    "track": {
                      "type": "boolean",
                      "default": false
                    }
                  },
                  "required": [
                    "track"
                  ]
                },
                "afk": {
                  "type": "object",
                  "properties": {
                    "ignore": {
                      "type": "boolean",
                      "default": true
                    }
                  },
                  "required": [
                    "ignore"
                  ]
                }
              },
              "required": [
                "join",
                "leave",
                "afk"
              ]
            }
          },
          "required": [
            "user",
            "message",
            "voice"
          ]
        },
        "user": {
          "type": "object",
          "properties": {
            "leave": {
              "type": "object",
              "properties": {
                "keep": {
                  "type": "boolean",
                  "default": true
                }
              },
              "required": [
                "keep"
              ]
            },
            "ban": {
              "type": "object",
              "properties": {
                "keep": {
                  "type": "boolean",
                  "default": true
                }
              },
              "required": [
                "keep"
              ]
            },
            "kick": {
              "type": "object",
              "properties": {
                "keep": {
                  "type": "boolean",
                  "default": true
                }
              },
              "required": [
                "keep"
              ]
            }
          },
          "required": [
            "leave",
            "ban",
            "kick"
          ]
        },
        "role": {
          "type": "object",
          "properties": {
            "admin": {
              "type": "array",
              "default": []
            },
            "ranks": {
              "type": "object",
              "default": {}
            }
          },
          "required": [
            "admin",
            "ranks"
          ]
        }
      },
      "required": [
        "event",
        "commands",
        "control",
        "user",
        "role",
        "egg_done"
      ]
    }
  },
  "required": [
    "logger",
    "bot"
  ]
}
